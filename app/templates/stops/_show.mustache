{{#destination}}
<p>
{{color}}
  <span class="number label label-default{{^active}} label-empty{{/active}}" style="background-color: {{#color}}{{color}}{{/color}}{{^color}}#707070{{/color}};">{{number}}</span>{{vehicle_name}}</br>
  <b>{{#ref}}{{ref}} {{/ref}}{{name}}</b></br>
  {{street}}</br>
  {{#detail}}
    {{detail}}</br>
  {{/detail}}
  {{postalcode}} {{city}}
</p>
<ul>
  {{#open}}<li>{{#i18n}}plannings.edit.popup.open{{/i18n}} {{open}}</li>{{/open}}
  {{#close}}<li>{{#i18n}}plannings.edit.popup.close{{/i18n}} {{close}}</li>{{/close}}
  {{#time}}<li> {{#i18n}}plannings.edit.popup.time{{/i18n}} <i class="fa fa-clock-o fa-fw"></i> {{time}}</li>{{/time}}
  {{#quantity}}<li>{{#i18n}}plannings.edit.popup.quantity{{/i18n}} <i class="fa fa-archive fa-fw"></i> {{quantity}}</li>{{/quantity}}
  {{#take_over}}<li>{{#i18n}}plannings.edit.popup.take_over{{/i18n}} <i class="fa fa-clock-o fa-fw"></i> {{take_over}}</li>{{/take_over}}
</ul>
{{#comment}}
  <p>{{comment}}</p>
{{/comment}}
{{#error}}
  <ul class="ui-state-error">
    {{#out_of_window}}
      <li><i class="fa fa-clock-o fa-fw"></i> {{#i18n}}plannings.edit.error.out_of_window{{/i18n}}</li>
    {{/out_of_window}}
    {{#out_of_capacity}}
      <li><i class="fa fa-archive fa-fw"></i> {{#i18n}}plannings.edit.error.out_of_capacity{{/i18n}}</li>
    {{/out_of_capacity}}
    {{#out_of_drive_time}}
      <li><i class="fa fa-power-off fa-fw"></i> {{#i18n}}plannings.edit.error.out_of_drive_time{{/i18n}}</li>
    {{/out_of_drive_time}}
  </ul>
{{/error}}
{{/destination}}
