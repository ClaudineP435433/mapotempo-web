<% if user_signed_in? %>
  <% if @customer %>
    <% if not @customer.end_subscription or @customer.end_subscription > Time.now %>
      <%= render 'subscription_on' %>
    <% else %>
      <%= render 'subscription_end' %>
    <% end %>
  <% end %>
<% else %>
  <%= render 'default_home_page' %>
<% end %>
